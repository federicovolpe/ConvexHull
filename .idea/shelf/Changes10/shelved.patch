ring toPolymake(List<Point2D> points){\n      StringBuilder sb = new StringBuilder(\"new Polytope(POINTS =>[\");\n      for (Point2D n: points) {\n        sb.append(n.toPolymakeVert()).append(\",\\n\");\n      }\n      return sb.substring(0,sb.length()-2)+ \"]);\";\n  }\n}\n
